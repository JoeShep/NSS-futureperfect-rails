<h1> Projects </h1>
<ul>
	<% @projects.each do |project| %>
		<li><%= project.name %></li>
	<% end %>
	<li>
	<%= form_for @project do |f| %>
		<% @project.errors.full_messages.each do |message| %>
			<p><%= message %></p>
		<% end %>
		<%= f.label :name, "Project Name" %>
		<%= f.text_field :name %>
		<%= f.submit "Add Project" %>
	<% end %>
	</li>
</ul>
